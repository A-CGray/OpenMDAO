[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

omit =
    */python?.?/*
    */site-packages/nose/*
    */test/*
    openmdao/code_review/*
    openmdao/devtools/*
    openmdao/test_suite/*
    openmado/utils/*